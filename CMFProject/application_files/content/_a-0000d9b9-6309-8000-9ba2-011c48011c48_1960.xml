<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_CreateRecordAssocDataDef</name>
        <uuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1960</uuid>
        <description>Creates a data definition entry for the selected record associations</description>
        <parentUuid>_a-0000da20-55d5-8000-9ba2-011c48011c48_11112</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=
'type!{urn:com:appian:types:CASE_APP}CASE_APP_RecordAssociation'(
  recordName: ri!recordName,
  recordUUID: ri!recordUUID,
  identifiers: ri!identifiers
)


/*{
   recordType: ri!recordType,
   recordName: ri!recordName,
   recordUUID: ri!recordUUID,
   identifiers: apply(rule!CASE_APP_IdentifiersArray(_), ri!identifiers)
}*/</definition>
        <namedTypedValue>
            <name>recordName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>recordUUID</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>identifiers</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
    </rule>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_SR_cropTextLength</name>
        <uuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2826</uuid>
        <description>If the passed in text is greater than the max length, the text will be cropped, and ellipsis will be added at the end.</description>
        <parentUuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2617</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(len(ri!text)&gt;ri!maxLength,
  left(ri!text,ri!maxLength)&amp;&quot;...&quot;,
  ri!text
)</definition>
        <namedTypedValue>
            <name>text</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>maxLength</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
    </rule>
    <roleMap public="true">

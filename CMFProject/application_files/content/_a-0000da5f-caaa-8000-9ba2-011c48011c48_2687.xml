<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_createUiMessage</name>
        <uuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2687</uuid>
        <description>Generates a rich text format message to be displayed on a SAIL Dashboard or Form.</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1981</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_richTextDisplayField&quot;(
  labelPosition: &quot;COLLAPSED&quot;,
  value: #&quot;SYSTEM_SYSRULES_richTextItem&quot;(
    text: ri!msg,
    style: &quot;HEADER_SMALL&quot;
  )
)</definition>
        <namedTypedValue>
            <name>msg</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
    </rule>
    <roleMap public="true">

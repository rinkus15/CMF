        <uuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2785</uuid>
        <description>Removes the selected document from the array of added docs</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1672</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=if(count(ri!array)=1,
  {cast(typeof(ri!array), null)},
  remove(ri!array, ri!index)
)</definition>
        <namedTypedValue>
            <name>array</name>
            <type>
                <name>Variant</name>

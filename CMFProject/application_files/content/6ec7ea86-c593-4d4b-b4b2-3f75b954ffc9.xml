    <rule>
        <name>CASE_APP_RecordPickerFromRecordAssociation</name>
        <uuid>6ec7ea86-c593-4d4b-b4b2-3f75b954ffc9</uuid>
        <description>Constructs a Record picker from a Record Association Object</description>
        <parentUuid>_a-0000d9f1-8353-8000-9ba2-011c48011c48_10351</parentUuid>

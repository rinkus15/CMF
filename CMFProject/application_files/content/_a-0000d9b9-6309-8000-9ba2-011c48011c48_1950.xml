<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_toLabelValue</name>
        <uuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1950</uuid>

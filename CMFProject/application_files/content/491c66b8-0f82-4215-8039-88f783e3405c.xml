<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_MultiDynamicRecordPicker</name>
        <uuid>491c66b8-0f82-4215-8039-88f783e3405c</uuid>
        <description>Generate multiple record pickers based on dynamically selected sources</description>
        <parentUuid>_a-0000d9f1-8353-8000-9ba2-011c48011c48_10351</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/* To test this - set ri!values to {} */
{
  #&quot;SYSTEM_SYSRULES_applyComponents&quot;( 
    function: #&quot;8b8bfc98-272d-4c58-8c40-85983cc70449&quot;(
        sectionLabel: ri!sectionLabel,
        labelPosition: ri!labelPosition,
        required: ri!required,
        requiredMessage: ri!requiredMessage,
        maxSelections: ri!maxSelections,
        readOnly: ri!readOnly,
        value: ri!values,
        index: _
    ),
    array: if(
      or(#&quot;_a-0000d9a9-eb56-8000-9e06-010000010000_751&quot;(ri!readOnly),ri!readOnly),
      1+fn!enumerate(1+count(ri!values))
      1+fn!enumerate(count(ri!values))
    )
  )
}

</definition>
        <namedTypedValue>
            <name>sectionLabel</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>labelPosition</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>required</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>requiredMessage</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>maxSelections</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>values</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>readOnly</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
    </rule>
